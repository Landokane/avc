Private Sub mnuSettingsIn_Click(Index As Integer)

If Index = 0 Then

    a$ = InputBox("Please enter your old password:", "Change Password")
    b$ = InputBox("Please enter your new password:", "Change Password")
       
    SendPacket "CP", a$ + Chr(250) + b$

End If

If Index = 1 Then

    If mnuSettingsIn(Index).Checked = True Then
        mnuSettingsIn(Index).Checked = False
    Else
        mnuSettingsIn(Index).Checked = True
    End If
End If

If Index = 2 Then

    If mnuSettingsIn(Index).Checked = True Then
        mnuSettingsIn(Index).Checked = False
    Else
        mnuSettingsIn(Index).Checked = True
    End If
End If

If Index = 3 Then
    If mnuSettingsIn(3).Checked = True Then
        mnuSettingsIn(3).Checked = False
    Else
        mnuSettingsIn(3).Checked = True
    End If
End If

If Index = 5 Then frmColors.Show
If Index = 7 Then frmHalfConfig.Show



End Sub

