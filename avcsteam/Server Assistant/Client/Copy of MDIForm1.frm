Private Sub Toolbar1_ButtonClick(ByVal Button As MSComctlLib.Button)

If Button.Index = 1 Then SendPacket "BS", ""
If Button.Index = 2 Then frmMap.Show
If Button.Index = 3 Then SendPacket "F0", "": frmFileBrowser.Show
If Button.Index = 4 Then
   
    If HLEXEPath = "" Then
        MsgBox "You must configure this feature first."
        frmHalfConfig.Show
        Exit Sub
    End If
   
   
    a$ = HLEXEPath + " -console " + HLExtraArgs + " -game " + HLGame + " +connect " + HLIP + ":" + HLPort
    'MsgBox a$

    Shell a$, vbNormalFocus
    
    If HLQuitSA = 1 Then End

End If


End Sub
